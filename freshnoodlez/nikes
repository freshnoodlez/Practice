
use_synth :piano
use_bpm 80

def nikes
  play :e2, duration: 4
  play :c2, duration: 4
end

def nikes2
  play :e4, duration: 2.0
  play :g4, duration: 2.0
  play :b5, duration: 2.0
end

def nikes3
  play :e4, duration: 6.0
  play :g4, duration: 6.0
  play :b5, duration: 6.0
  sleep 1
end

def nikes4
  play :g3, duration: 5
  play :g2, duratino: 5
end
